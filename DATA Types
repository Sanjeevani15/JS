      <script> 
      var num=5;
      document.write(typeof(num)+ "\n\n");

      var n="5";
      document.write(typeof(n)+ "\n\n");
      
      var bool=true;
      document.write(typeof(bool)+ "\n\n");
      
      var myvar;
      document.write(typeof(myvar)+ "\n\n");

      var myvar=null;
      document.write(typeof(myvar)+ "\n\n");

     // -----------------------------------------

      var num1=56789034567n;  //here n in the  number specifies that this is a type of big int 
      //genrally used to display big integer number
      document.write(typeof(num1)+ "\n\n");
      



    </script>
