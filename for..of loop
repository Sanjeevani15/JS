 //LOOPS in JAVASCRIPT
    
    //for..of loop statement

    //STRINGS
    
    //one element one line
    let lang = "JavaScript";
    for (let x of lang) {
    document.write(x , "<br>");
    }
    
    
    
    
    
    //add one element ine evry line
    let lang = "JavaScript";
    let text = "";
    for (let x of lang) {
    document.write(text += x , "<br>");
    }
    
    //ARRAYS

    let names=["sanju" , "sejal ", "sanjeevani", "sam" ];
    for (let i of names){
       document.write(i +"<br>");   
       
    }
    
    
    
  
    
